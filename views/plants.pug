html
  head
    title Plants List
    link(rel='stylesheet', href='/styles.css')  // Link to external CSS file
  body
    h1 Plants List
    each plant in plants
      .plant
        .plant-name= plant.plant_name
        .plant-type= "Type: " + plant.plant_type
        .plant-age= "Age: " + plant.plant_age + " years"
